      </input>
      <output id="OutputClause_071hp49" label="Валідний сертифікат" name="certificateDateValidationPass" typeRef="boolean" biodi:width="192" />
      <rule id="DecisionRule_1w4iw1r">
        <inputEntry id="UnaryTests_1i5p4gg">
