    <bpmn:scriptTask id="Activity_1ap9x8j" name="Підготовка даних для запису (transient var)" scriptFormat="groovy" camunda:resultVariable="payload">
      <bpmn:incoming>Flow_06zm02c</bpmn:incoming>
      <bpmn:outgoing>Flow_0u9qq1c</bpmn:outgoing>
      <bpmn:script>import static org.camunda.spin.Spin.JSON

import org.camunda.bpm.engine.variable.Variables
import java.time.LocalDateTime
import java.time.format.DateTimeFormatter

var formData = JSON(signedFormDataOutput).prop("data")
var requestMap = [:]

for (var name : formData.fieldNames()) {
  requestMap[name] = formData.prop(name).prop("value").value()
}

if (requestMap.accreditationEndDate?.trim()) {
  def parsedDate = LocalDateTime.parse(requestMap.accreditationEndDate, DateTimeFormatter.ISO_OFFSET_DATE_TIME)

  requestMap.accreditationEndDate = parsedDate.format(DateTimeFormatter.ofPattern("yyyy-MM-dd"))
}

var payload = Variables.stringValue(JSON(requestMap).toString(), true)</bpmn:script>
